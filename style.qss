QCheckBox {
    color: white;
    font-size: 14px;
    font-weight: 500;
    font-family: Segoe UI Light;
}

QCheckBox::indicator {
    border-radius: 3px;
    border: 1px solid rgb(240, 243, 247);
}

QCheckBox::indicator:off {
    background-color: rgb(20,23,26);
}

QCheckBox::indicator:on {
    background-color: rgb(238,174,51);
}

QComboBox {
    background-color: rgb(238,174,51);
    border-radius: 5px;
    padding: 7px;
    font-size: 16px;
    font-weight: 400;
    font-family: Segoe UI Light;
    color: white;
}

QComboBox:hover {
    background-color: rgb(248,184,61);
}

QComboBox::drop-down:closed {
    image: url(":/helper/helper_icons/closed.png");
    width: 50px;
    height: 50px;
}

QComboBox::drop-down:open {
    image: url(":/helper/helper_icons/open.png");
    width: 50px;
    height: 50px;
}

