/* ------------------- MAIN TABS ----------------- */

QWidget#MAIN_WIDGET {
    background-color: rgb(43, 46, 49);
    border-color: rgb(43, 46, 49);
}

QTabBar {
    background: transparent;
    border-color: rgb(43, 46, 49);
}

QTabBar::tab:!selected:hover {
    background-color: rgb(46,171,194);
    border: 2px dashed white;
}

QTabBar::tab {
   background-color: rgb(63, 66, 69);
   border-top-left-radius: 8px;
   border-bottom-left-radius: 8px;
   border-color: rgb(43, 46, 49);
   margin-bottom: 3px;
   margin-right: 1px;
   font-size: 30px;
   font-weight: 400;
   font-family: Segoe UI Light;
   color: white;
   letter-spacing: 1px;
}

QTabBar::tab:selected {
   background-color: rgb(238,174,51);
   border-left: 2px solid white;
   border-top: 2px solid white;
   border-bottom: 2px solid white;
   font-weight: 600;
}

QWidget#SEARCH_TAB, QWidget#ADD_TAB, QWidget#DETAIL_TAB, QWidget#TABLE_TAB {
    background-color: rgb(43, 46, 49);
}

QTabWidget,
QTabWidget QWidget {
    background-color: rgb(43, 46, 49);
    border-color: rgb(43, 46, 49);
}

QTabWidget QWidget QWidget,
QTabWidget QWidget QWidget QWidget {
    background-color: transparent;
    border-color: rgb(43, 46, 49);
}

